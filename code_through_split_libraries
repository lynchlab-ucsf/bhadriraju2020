gunzip Undetermined_S0_L00*

/data/Users/dfadrosh/scripts/FastQC/fastqc --nogroup *.fastq

mkdir FASTQC

mv *_fastqc* FASTQC

flash -r 153 -f 253 -s 30 -m 25 -d ./Output_folder_L001/ -q Undetermined_S0_L001_R1_001.fastq Undetermined_S0_L001_R2_001.fastq
flash -r 153 -f 253 -s 30 -m 25 -d ./Output_folder_L002/ -q Undetermined_S0_L002_R1_001.fastq Undetermined_S0_L002_R2_001.fastq
flash -r 153 -f 253 -s 30 -m 25 -d ./Output_folder_L003/ -q Undetermined_S0_L003_R1_001.fastq Undetermined_S0_L003_R2_001.fastq
flash -r 153 -f 253 -s 30 -m 25 -d ./Output_folder_L004/ -q Undetermined_S0_L004_R1_001.fastq Undetermined_S0_L004_R2_001.fastq 

sed -n '1~4'p ./Output_folder_L001/out.extendedFrags.fastq > ./Output_folder_L001/FLAShReads.txt
sed -n '1~4'p ./Output_folder_L002/out.extendedFrags.fastq > ./Output_folder_L002/FLAShReads.txt
sed -n '1~4'p ./Output_folder_L003/out.extendedFrags.fastq > ./Output_folder_L003/FLAShReads.txt
sed -n '1~4'p ./Output_folder_L004/out.extendedFrags.fastq > ./Output_folder_L004/FLAShReads.txt

extract_barcodes.py -f Undetermined_S0_L001_R1_001.fastq -c barcode_in_label -s ':' -l 12 -o ./Output_folder_L001/extracted_barcodes_L001/
extract_barcodes.py -f Undetermined_S0_L002_R1_001.fastq -c barcode_in_label -s ':' -l 12 -o ./Output_folder_L002/extracted_barcodes_L002/
extract_barcodes.py -f Undetermined_S0_L003_R1_001.fastq -c barcode_in_label -s ':' -l 12 -o ./Output_folder_L003/extracted_barcodes_L003/
extract_barcodes.py -f Undetermined_S0_L004_R1_001.fastq -c barcode_in_label -s ':' -l 12 -o ./Output_folder_L004/extracted_barcodes_L004/

cp /data/Users/dfadrosh/scripts/filter_fasta_keep_order.py ./Output_folder_L001/extracted_barcodes_L001/
python Output_folder_L001/extracted_barcodes_L001/filter_fasta_keep_order.py Output_folder_L001/extracted_barcodes_L001/barcodes.fastq Output_folder_L001/FLAShReads.txt 
mv Index_filtered_ordered.fastq /data/NextSeq_data/NextSeq_Processed/160826_NS500170_0043_AHG5VKBGXY/Output_folder_L001/Index_filtered_ordered_L001.fastq

cp /data/Users/dfadrosh/scripts/filter_fasta_keep_order.py ./Output_folder_L002/extracted_barcodes_L002/
python Output_folder_L002/extracted_barcodes_L002/filter_fasta_keep_order.py Output_folder_L002/extracted_barcodes_L002/barcodes.fastq Output_folder_L002/FLAShReads.txt 
mv Index_filtered_ordered.fastq /data/NextSeq_data/NextSeq_Processed/160826_NS500170_0043_AHG5VKBGXY/Output_folder_L002/Index_filtered_ordered_L002.fastq

