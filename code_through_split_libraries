split_libraries_fastq.py -i Output_folder_L002/out.extendedFrags.fastq -b Output_folder_L002/Index_filtered_ordered_L002.fastq -o Output_folder_L002/splibs_L002  -m NextSeq_07122016_mapping.txt --rev_comp_mapping_barcodes --store_demultiplexed_fastq -r 0 -q 0 -n 100 -p 0.1 --barcode_type 12  
split_libraries_fastq.py -i Output_folder_L003/out.extendedFrags.fastq -b Output_folder_L003/Index_filtered_ordered_L003.fastq -o Output_folder_L003/splibs_L003  -m NextSeq_07122016_mapping.txt --rev_comp_mapping_barcodes --store_demultiplexed_fastq -r 0 -q 0 -n 100 -p 0.1 --barcode_type 12  
split_libraries_fastq.py -i Output_folder_L004/out.extendedFrags.fastq -b Output_folder_L004/Index_filtered_ordered_L004.fastq -o Output_folder_L004/splibs_L004  -m NextSeq_07122016_mapping.txt --rev_comp_mapping_barcodes --store_demultiplexed_fastq -r 0 -q 0 -n 100 -p 0.1 --barcode_type 12  

cat Output_folder_L001/splibs_L001/seqs.fastq Output_folder_L002/splibs_L002/seqs.fastq Output_folder_L003/splibs_L003/seqs.fastq Output_folder_L004/splibs_L004/seqs.fastq > seqs.fastq

rm Undetermined*fastq 

usearch -fastq_filter seqs.fastq  -fastq_maxee 2 --eeout -fastaout quality_seqs.fasta

~~~~~~~Generation of representatively OTU table~~~~~~~
mkdir /data/Users/dfadrosh/CORE/METCALFE

cd /data/Users/dfadrosh/CORE/METCALFE

filter_fasta.py -f /data/NextSeq_data/NextSeq_Processed/160826_NS500170_0043_AHG5VKBGXY/quality_seqs.fasta --sample_id_fp keep.list -o quality_seqs.fasta

perl /data/Users/dfadrosh/scripts/drive5/bmp_uparse_pipeline.pl -i quality_seqs.fasta -o reads_uparse.fa

usearch --derep_fulllength reads_uparse.fa -sizeout -output derep.fa -minuniquesize 2
###############################################
6:05 20.5Gb  100.0% Reading reads_uparse.fa
27:28 24.1Gb 11235959 (11.2M) uniques, avg cluster 5.4, median 1, max 1813766
1743463 uniques written, 9492496 clusters size < 2 discarded (84.5%)
###############################################

/data/Software/usearch8.0.1623_i86linux32 -sortbysize derep.fa -fastaout sorted.fa -minsize 2 
###############################################
00:06 598Mb  100.0% Reading derep.fa
00:06 564Mb Getting sizes           
00:10 578Mb Sorting 1743463 sequences
00:17 585Mb  100.0% Writing output
###############################################

/data/Software/usearch8.0.1623_i86linux32 -cluster_otus sorted.fa -sizein -sizeout -otus otus.fa -uparseout otu_clustering.txt
###############################################
06:08  49Mb  100.0% 1042 OTUs, 371554 chimeras (21.3%)
###############################################

python /data/Users/dfadrosh/scripts/drive5/fasta_number.py otus.fa OTU_ > otus_numbered.fa

usearch -usearch_global reads_uparse.fa -db otus_nonchimeras_numbered.fa -strand plus -id 0.97 -uc map.uc
###############################################
00:01  19Mb Reading otus_nonchimeras_numbered.fa, 243.9kb
00:01  19Mb 921 seqs, min 155, avg 252, max 266nt
00:01  19Mb  100.0% Masking
00:01  20Mb  100.0% Word stats
00:01  22Mb  100.0% Building slots
00:01  22Mb  100.0% Build index   
09:20 2.8Gb  100.0% Searching, 88.9% matched
###############################################

python /data/Users/dfadrosh/scripts/drive5/uc2otutab.py map.uc > otu_table.txt

biom convert -i otu_table.txt -o otu_table_uparse.biom --to-hdf5 --table-type="OTU table"

####Align OTU fasta files in preparation of tree making
parallel_align_seqs_pynast.py -i otus_numbered.fa -o pynast_aligned_seqs -t /data/Users/dfadrosh/CORE/scripts/core_set_aligned.fasta.imputed -T -O 30

####Filter alignment to remove gaps
filter_alignment.py -i pynast_aligned_seqs/otus_numbered_aligned.fasta -m /data/Users/dfadrosh/CORE/scripts/lanemask_in_1s_and_0s -o pynast_aligned_seqs/pynast_filtered

####Create the Phylogenetic Tree
make_phylogeny.py -i pynast_aligned_seqs/pynast_filtered/otus_numbered_aligned_pfiltered.fasta -o otus_labeled_aligned_pfiltered.tre

###Use grep to create a text file containing all of the OTUs that did not align.
grep "OTU" pynast_aligned_seqs/otus_numbered_failures.fasta > pynast_aligned_seqs/otu.exclude.list

####Remove the ">" from each line of the text file.
cut -c 2- pynast_aligned_seqs/otu.exclude.list > pynast_aligned_seqs/otu.exclude.cleaned.list

####filter biom OTU table to remove any OTUs that did not align, and include only OTUs that are present at at a threshold value or percentage.  Here I am filtering to 1/10,000 of a percent which in my case removed OTUs that have total read numbers less than ~600
filter_otus_from_otu_table.py -i otu_table_uparse.biom -o otu_table_uparse_0.000001_filtered.biom --min_count_fraction 0.000001 -s 1 -e pynast_aligned_seqs/otu.exclude.cleaned.list

####Assign Taxonomy
assign_taxonomy.py -i otus_numbered.fa -o uchime_taxonomy -r /data/Greengenes_Database_May_2013/gg_13_5_otus/rep_set/97_otus.fasta -t /data/Greengenes_Database_May_2013/gg_13_5_otus/taxonomy/97_otu_taxonomy.txt

####Add taxonomy to biom file
biom add-metadata -i otu_table_uparse_0.000001_filtered.biom -o otu_table_uparse_0.000001_filtered_tax.biom --observation-metadata-fp uchime_taxonomy/otus_numbered_tax_assignments.txt --observation-header OTUID,taxonomy,confidence --sc-separated taxonomy --float-fields confidence

####Convert biom to txt
biom convert -i otu_table_uparse_0.000001_filtered_tax.biom -o otu_table_uparse_0.000001_filtered_tax.txt --to-tsv --header-key taxonomy --table-type "OTU table"

####Create alpha rarefaction curves to assess read depth/rarefaction
alpha_rarefaction.py -i otu_table_uparse_0.000001_filtered_tax.biom -o alpha_rarefaction -m /data/NextSeq_data/NextSeq_Processed/160826_NS500170_0043_AHG5VKBGXY/Alice_mapping_08312016.txt -f -p /data/Users/dfadrosh/CORE/scripts/qiime_parameters_diversity.txt -t otus_labeled_aligned_pfiltered.tre

biom summarize-table -i otu_table_uparse_0.000001_filtered_tax.biom -o otu_table_uparse_0.000001_filtered_tax.biom.summary

~~~~~selecting multiple rarefying depth of 58978~~~~~

####Run reprare script using bray curtis and 250 tables

Rscript Multiply_Rarefy.R

biom convert -i otu_table_uparse_0.000001_filtered_tax_58978reprare_bray_250tables.txt -o otu_table_uparse_0.000001_filtered_tax_58978reprare_bray_250tables.biom --to-hdf5 --table-type "OTU table" --process-obs-metadata taxonomy



